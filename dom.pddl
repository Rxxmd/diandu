( DEFINE ( DOMAIN ELECTROPLATING )
( :REQUIREMENTS :EQUALITY :CONDITIONAL-EFFECTS :TYPING )
( :TYPES
	POLE - OBJECT
	SLOT - OBJECT
	PRODUCT - OBJECT
)
( :PREDICATES
	( POLE_POSITION ?POLE0 - POLE ?SLOT1 - SLOT )
	( SLOT_HAVE_POLE ?SLOT0 - SLOT )
	( POLE_REGION ?POLE0 - POLE ?SLOT1 - SLOT )
	( POLE_HAVE_THINGS ?POLE0 - POLE ?PRODUCT1 - PRODUCT )
	( POLE_EMPTY ?POLE0 - POLE )
	( POLE_AVAILABLE ?POLE0 - POLE )
	( PRODUCT_AT ?PRODUCT0 - PRODUCT ?SLOT1 - SLOT )
	( TARGET_SLOT ?SLOT0 - SLOT ?PRODUCT1 - PRODUCT )
	( GEAR_POLE ?POLE0 - POLE )
	( FORWARD_SLOT_CONNECTION ?SLOT0 - SLOT ?SLOT1 - SLOT )
	( INVERSE_SLOT_CONNECTION ?SLOT0 - SLOT ?SLOT1 - SLOT )
	( EXCHANGING_CONNECTION ?SLOT0 - SLOT ?SLOT1 - SLOT )
	( BORDER_SLOT ?SLOT0 - SLOT )
	( STOCKING_SLOT ?SLOT0 - SLOT )
	( BLANKING_SLOT ?SLOT0 - SLOT )
	( EXCHANGING_SLOT ?SLOT0 - SLOT )
	( SLOT_NOT_AVAILABLE ?SLOT0 - SLOT )
	( POLE_START_MOVING ?POLE0 - POLE )
	( POLE_STOP_MOVING ?POLE0 - POLE )
	( EMPTYSTACK )
)
( :ACTION DO-START-MOVING-POLE
  :PARAMETERS ( ?POLE0 - POLE )
  :PRECONDITION
	( AND
		( POLE_STOP_MOVING ?POLE0 )
		( POLE_AVAILABLE ?POLE0 )
		( EMPTYSTACK )
	)
  :EFFECT
	( AND
		( POLE_START_MOVING ?POLE0 )
		( POLE_AVAILABLE ?POLE0 )
		( NOT ( POLE_STOP_MOVING ?POLE0 ) )
	)
)
